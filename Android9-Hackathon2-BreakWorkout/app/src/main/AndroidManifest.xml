<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="techkids.com.android9_hackathon2_breakworkout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.PracticeScene" />
        <activity android:name=".views.AlarmScene" />

        <service android:name=".BroadcastService" />

        <activity
            android:name=".views.FinishScene"
            android:label="@string/title_activity_finish_scene"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
    </application>

</manifest>